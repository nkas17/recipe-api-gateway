    "jsonwebtoken": "8.5.1",
    "mongoose": "6.2.6",
    "morgan": "1.10.0",
    "request": "2.88.2",
    "request-promise": "4.2.6"
  },
  "devDependencies": {
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
